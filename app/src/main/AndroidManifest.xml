<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cardiomais">
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/heart"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/heart"
        android:supportsRtl="true"
        android:theme="@style/Theme.CardioMais">
        <service android:name=".BtService">

        </service>
        <service android:name=".CallService"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ListaDisp"
            android:label="Dispositivos"
            android:theme="@android:style/Theme.Dialog"
        />

        <activity android:name=".ListaContatos"
            android:label="Adicionar contato"
            android:theme="@style/Theme.CardioMais"
            />

    </application>

</manifest>